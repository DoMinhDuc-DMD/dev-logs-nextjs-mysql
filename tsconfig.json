{
  "compilerOptions": {
    "target": "ES2017",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "allowImportingTsExtensions": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": [
        "./*"
      ]
    },
    "noImplicitAny": false
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts",
    ".next/types/app/api/AccountCreate/route.tsx",
    ".next/types/app/api/AccountDetail/route.tsx",
    ".next/types/app/api/AccountList/route.tsx",
    ".next/types/app/api/AccountSetting/route.tsx",
    ".next/types/app/api/Auth/route.tsx",
    ".next/types/app/api/Dashboard/route.tsx",
    ".next/types/app/api/DevlogHistory/route.tsx",
    ".next/types/app/api/DevlogInput/route.tsx",
    ".next/types/app/api/DevlogList/route.tsx",
    ".next/types/app/api/Layout/route.tsx",
    ".next/types/app/api/ProjectAdd/route.tsx",
    ".next/types/app/api/ProjectList/route.tsx",
    "app/api/AccountSetting/route.ts",
    "app/api/Auth/route",
    "app/api/ConnectDB/db.ts",
    "next-env.d.ts",
    "next.config.js",
    "dist/types/**/*.ts",
    "app/api/Dashboard/route.ts",
    "app/api/sqlite/sqliteseed.ts",
    "app/api/sqlite/sqlitedb.ts",
  ],
  "exclude": [
    "node_modules"
  ]
}